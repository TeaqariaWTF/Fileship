        "postinstall": "nuxt prepare",
        "lint": "eslint . && prettier --check .",
        "lint:fix": "eslint --fix . && prettier --write .",
        "check": "pnpm build && nuxt typecheck && pnpm lint",
        "release": "changelogen --release"
