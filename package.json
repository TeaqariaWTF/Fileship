{
    "name": "fileship",
    "description": "Fileship is an open-source and self-hosted image uploading service with cool features.",
    "version": "1.5.0",
    "type": "module",
    "scripts": {
        "build": "prisma generate && nuxt build",
        "dev": "prisma db push && nuxt dev -o",
